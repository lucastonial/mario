      stereotype "source"
      cpp_src "int main(int argc, char **argv)
{
    CGame game(5,30);

	game.init(\"Framework basico com estados\",640,480,32,false);

    game.changeState(MenuState::instance());

	while(game.isRunning())
	{
		game.handleEvents();
		game.update();
		game.draw();
	}

	// cleanup the engine
	game.clean();
}
